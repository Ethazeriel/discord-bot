{
    "debug": false,
    "functions": {
      "web": true,
      "music": true,
      "translate": true
    },
    "root_url": "http://localhost",
    "discord": {
        "token": "yourbotaccesstoken",
        "client_id": "yourbotclientid",
        "secret": "yourbotclientsecret",
        "guildId": "testguildid",
        "scope": "guild/global",
        "roles": {
            "dj":"DJ",
            "admin":"Admin",
            "translate":"Limited Participation"
        },
        "redirect_uri":"http://localhost/oauth2?type=discord"
    },
    "spotify": {
        "client_id": "yourclientid",
        "client_secret": "yourclientsecret",
        "redirect_uri":"http://localhost/oauth2?type=spotify"
    },
    "napster": {
        "client_id": "yourclientid",
        "client_secret": "yourclientsecret",
        "redirect_uri": "http://localhost/oauth2?type=napster"
    },
    "lastfm": {
        "client_id": "yourclientid",
        "client_secret": "yourclientsecret",
        "redirect_uri": "http://localhost/basicauth?type=lastfm"
    },
    "youtube": {
        "apiKey": "youryoutubeapikey",
        "useragent": "yourbrowseruseragentstring"
    },
    "subsonic": {
        "username": "username",
        "password": "password",
        "client_id": "goose",
        "endpoint_uri": "http://subsonic.example.com",
        "regex": "(?:subsonic\\.example\\.com:4533|192\\.168\\.0\\.1:4533)(?:\\\/app\\\/#?\\\/)((?:track|playlist|album){1})(?:\\\/)([a-f0-9-]{32,36})(?:\\\/show)"
    },
    "mongo": {
        "url": "mongodb://username:password@localhost:27017/?authSource=admin",
        "database": "test",
        "trackcollection": "tracks",
        "usercollection": "users"
    },
    "translate": {
        "apiKey": "yourcloudtranslateapikey"
      }
